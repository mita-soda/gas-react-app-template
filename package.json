        "@types/react-router-dom": "5.1.4",
        "html-webpack-inline-source-plugin": "1.0.0-beta.2",
        "html-webpack-plugin": "4.2.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-hook-form": "6.3.0",
        "react-router-dom": "5.1.2",
        "ts-loader": "6.2.2",
        "typescript": "3.8.3",
        "webpack": "4.42.1",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "3.10.3"
    }
